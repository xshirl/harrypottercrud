<% include ../../partials/boilerplate %>

<div class="student">
  <h1><%=student.fname%><%=student.lname%></h1>
  <img src="<%=student.image%>" width="200" alt="<%=student.fname%><%=student.lname%>" />
  <br>
   <form action="/students/<%=student.id%>/edit" class='put' method="GET">
  <input type="submit" class="editlink" value="Edit">
  </form>
  <br>
  <form action="/students/<%=student.id%>?_method=DELETE" class='delete' method="POST">
  <input type="submit" class="dellink" value="Delete">
  </form>
</div>

<script src='../../public/js/script.js'></script>
<% include ../../partials/end %>
